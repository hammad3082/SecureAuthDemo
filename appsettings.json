{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=MyAppDb;Trusted_Connection=True;"
  },
  "JwtSettings": {
    "Key": "YOUR_KEY_HERE", // placeholder, do not use in production
    "Issuer": "SecureAuthDemo",
    "Audience": "SecureAuthDemoUsers",
    "ExpiryMinutes": 60
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "RedisSettings": {
    "Host": "localhost",
    "Port": 6379,
    "Password": ""
  },
  "AllowedHosts": "*"
}